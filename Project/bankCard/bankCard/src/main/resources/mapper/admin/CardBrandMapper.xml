<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.soft.bankCard.Admin.Mapper.CardBrandMapper">


    <select id="selectCardBrandList" resultType="CardBrandVO">
        SELECT BRAND_SEQ, BRAND_NAME
        FROM T_CARD_BRAND
        WHERE BRAND_STATE = 'Y'
    </select>


</mapper>