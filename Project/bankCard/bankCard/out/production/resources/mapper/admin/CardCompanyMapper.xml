<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.soft.bankCard.Admin.Mapper.CardCompanyMapper">


    <select id="selectCardCompanyList" resultType="CardCompanyVO">
        SELECT CARD_COMPANY_SEQ, CARD_COMPANY_NAME
        FROM T_CARD_COMPANY
        WHERE CARD_COMPANY_STATE = 'Y'
    </select>


</mapper>